ion modbus tcp con la ip del esclavo y puerto del esclavo\n\t\n\t#(ID,Codigo,Inicio,Longitud)\n\tresultado_OUT=master.execute(1, cst.READ_HOLDING_REGISTERS, 9, 8)  # Resultado_OUT, recoge el valor de la lectura de los registros del 17 al 8 \n\t\n\t# Es un diccionario que recoge la lectura de cada registro para mandarlo posteriormente\n\tdic2 = {"Q0":resultado_OUT[0],"Q1":resultado_OUT[1],"Q2":resultado_OUT[2],"Q3":resultado_OUT[3],"Q4":resultado_OUT[4],"Q5":resultado_OUT[5],"Q6":resultado_OUT[6],"Q7":resultado_OUT[7]}\n\t\n\t# Retornamos el diccionario dic2\t\t\t\t\t\t\n\treturn dic2\n\n\n# Funcion de escritura de salidas de PLC\t\ndef Escritura_OUT():\n    \n\t# ESCRITURA DE SALIDAS        host="192.168.1.10",\n\tmaster = modbus_tcp.TcpMaster(host="192.168.1.10",port=5000)  # Conexion modbus tcp con la ip del esclavo y puerto del esclavo\n\t\n\t# Recogemos en la variable numero_IN el dato que nos envia cuando pulsamos un boton en la aplicacion, \n\t# buscamos la clave de la variable N_IN, que contiene el valor que necesitamos\n\tnumero_IN=int(request.vars.N_IN)\n\t\n\t# Si nos recibimos un 0\n\tif numero_IN==0:\n\t    \n\t    # La variable valor_IN0, recoge el valor de la busqueda de los datos recibidos de la clave Q0\n\t    valor_IN0=int(request.vars.Q0)\n\t    \n\t    # Escribimos en el registro 17 el valor que hemos recibido en la clave Q0 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 16, output_value=valor_IN0)\n\t\n\t# Si nos recibimos un 1\n\tif numero_IN==1:\n\t    \n\t    # La variable valor_IN1, recoge el valor de la busqueda de los datos recibidos de la clave Q1\n\t    valor_IN1=int(request.vars.Q1)\n\t    \n\t    # Escribimos en el registro 18 el valor que hemos recibido en la clave Q1 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 17, output_value=valor_IN1)\n\t\n\t# Si nos recibimos un 2\n\tif numero_IN==2:\n\t    \n\t    # La variable valor_IN2, recoge el valor de la busqueda de los datos recibidos de la clave Q2\n\t    valor_IN2=int(request.vars.Q2)\n\t    \n\t    # Escribimos en el registro 19 el valor que hemos recibido en la clave Q2\n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 18, output_value=valor_IN2)\n\t\n\t# Si nos recibimos un 3\n\tif numero_IN==3:\n\t    \n\t    # La variable valor_IN3, recoge el valor de la busqueda de los datos recibidos de la clave Q3\n\t    valor_IN3=int(request.vars.Q3)\n\t    \n\t    # Escribimos en el registro 20 el valor que hemos recibido en la clave Q3 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 19, output_value=valor_IN3)\n\t\n\t# Si nos recibimos un 4\n\tif numero_IN==4:\n\t    \n\t    # La variable valor_IN4, recoge el valor de la busqueda de los datos recibidos de la clave Q4\n\t    valor_IN4=int(request.vars.Q4)\n\t    \n\t    # Escribimos en el registro 21 el valor que hemos recibido en la clave Q4\n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 20, output_value=valor_IN4)\n\t\n\t# Si nos recibimos un 5\n\tif numero_IN==5:\n\t    \n\t    # La variable valor_IN5, recoge el valor de la busqueda de los datos recibidos de la clave Q5\n\t    valor_IN5=int(request.vars.Q5)\n\t    \n\t    # Escribimos en el registro 22 el valor que hemos recibido en la clave Q5 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 21, output_value=valor_IN5)\n\t\n\t# Si nos recibimos un 6    \n\tif numero_IN==6:\n\t    \n\t    # La variable valor_IN6, recoge el valor de la busqueda de los datos recibidos de la clave Q6\n\t    valor_IN6=int(request.vars.Q6)\n\t    \n\t    # Escribimos en el registro 23 el valor que hemos recibido en la clave Q6 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER, 22, output_value=valor_IN6)\n\t\n\t# Si nos recibimos un 7    \n\tif numero_IN==7:\n\t    \n\t    # La variable valor_IN7, recoge el valor de la busqueda de los datos recibidos de la clave Q7\n\t    valor_IN7=int(request.vars.Q7)\n\t    \n\t    # Escribimos en el registro 24 el valor que hemos recibido en la clave Q7 \n\t    master.execute(1, cst.WRITE_SINGLE_REGISTER,